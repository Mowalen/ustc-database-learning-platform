# .env 文件示例配置
# 请根据实际情况修改

# 项目基本配置
PROJECT_NAME="USTC Database Learning Platform"
API_V1_STR="/api/v1"

# MySQL 数据库配置
# 格式: mysql+aiomysql://用户名:密码@主机:端口/数据库名
DATABASE_URL=mysql+aiomysql://root:password@localhost:3306/ustc_learning_platform

# JWT 安全配置
SECRET_KEY=your-secret-key-here-please-change-this-to-something-secure
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# 调试模式
DEBUG=True
